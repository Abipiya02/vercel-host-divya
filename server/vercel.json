{
  "version": 2,
  "builds": [
      {
          "src":"index.js",
          "use": "@vercel/node"
      },{
        "src":"src/**/*",
        "use":"@vercel/static"
      }
  ],
  "routes":[
    {
      "src":"/(.*)",
      "dest":"/"
    }
  ],
  "headers": [
      {
        "source": "/(.*)",
        "headers": [
          { "key": "Access-Control-Allow-Origin", "value": "*" }
        ]
      },
      {
        "source": "/vercel_app_domain_name/(.*)",
        "headers": [
          { "key": "Access-Control-Allow-Origin", "value": "*" }
        ]
      }  
  ]
}